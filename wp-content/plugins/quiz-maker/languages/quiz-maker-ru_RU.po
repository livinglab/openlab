msgid "Disable"
msgstr "Отключить"

#: ../admin/partials/quizes/actions/quiz-maker-quizes-actions.php:3404
msgid "Form Fields"
msgstr "Поля Формы"

#: ../admin/partials/quizes/actions/quiz-maker-quizes-actions.php:3449
msgid "Required Fields"
msgstr "Обязательные Поля"

#: ../admin/partials/quizes/actions/quiz-maker-quizes-actions.php:3502
msgid "Add custom fields"
msgstr "Добавить пользовательские поля"

#: ../admin/partials/quizes/actions/quiz-maker-quizes-actions.php:3503
