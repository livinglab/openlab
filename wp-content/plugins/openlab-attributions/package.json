{
  "private": true,
  "name": "openlab-attributions",
  "description": "OpenLab text and images attribution plugin.",
  "version": "1.0.0",
  "author": "George Mamadashvili",
  "license": "GPL-2.0+",
  "main": "dist/js/block-editor.js",
  "scripts": {
    "build": "npm run prod",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch:sync": "cross-env sync=1 NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:styles": "cross-env stylelint './dist/scss/**/*.scss' --syntax scss",
    "lint:scripts": "cross-env eslint './dist/js/**/*.js'",
    "lint:scripts:fix": "cross-env eslint './dist/js/**/*.js' --fix"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "nanoid": "^2.1.1",
    "react-tippy": "^1.2.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^1.1.0",
    "@wordpress/eslint-plugin": "^2.3.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "2.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.2",
    "laravel-mix": "^4.0.15",
    "postcss-preset-env": "^6.6.0",
    "resolve-url-loader": "3.1.0",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "stylelint": "^13.2.1",
    "stylelint-config-wordpress": "^16.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
